{
	"name": "ecmascript-toolkit",
	"short_name": "ek",
	"version": "1.0.0-beta.0",
	"description": "",
	"scripts": {
		"start": "yarn serve",
		"start:analyze": "yarn analyze",
		"start:analyze:static": "yarn analyze:static",
		"start:prod": "yarn prod",
		"start:static": "yarn serve:static",
		"docs": "yarn esdoc",
		"clean:all": "yarn clean:dist && yarn clean:lib",
		"clean:install": "yarn rimraf node_modules && yarn install",
		"clean:dist": "yarn rimraf dist",
		"clean:lib": "yarn rimraf lib",
		"build": "yarn parallel --",
		"prod": "parallel-webpack -- --env.production=true",
		"legacy": "parallel-webpack -- --env.legacy=true",
		"deploy": "parallel-webpack -- --env.legacy=false --env.production=true --env.analyze=false --env.static=false",
		"parallel": "parallel-webpack --",
		"serve": "webpack server",
		"serve:prod": "webpack server --env.production=true --env.server=true",
		"serve:static": "parallel-webpack -- --env.production=false --env.server=false --env.static=true  && http-server ./dist",
		"analyze": "webpack server --env.analyze=true --env.server=true",
		"analyze:static": "parallel-webpack -- --env.analyze=true --env.static=true ",
		"analyze:manual": "parallel-webpack --profile --json > stats.json && yarn webpack-bundle-analyzer",
		"library": "yarn clean:lib && node ./config/script.minify.js",
		"watch": "yarn build --watch"
	},
	"author": "Ryan Tyler Spice-Finnie <spice.ryan@hotmail.com>",
	"private": true,
	"license": "MIT",
	"peerDependencies": {
		"webpack-cli": "latest"
	},
	"dependencies": {
		"mini-css-extract-plugin": "latest",
		"terser-webpack-plugin": "latest",
		"@babel/core": "latest",
		"@babel/plugin-proposal-class-properties": "latest",
		"@babel/plugin-proposal-decorators": "latest",
		"@babel/plugin-proposal-export-default-from": "latest",
		"@babel/plugin-proposal-export-namespace-from": "latest",
		"@babel/plugin-proposal-function-sent": "latest",
		"@babel/plugin-proposal-json-strings": "latest",
		"@babel/plugin-proposal-numeric-separator": "latest",
		"@babel/plugin-proposal-optional-chaining": "latest",
		"@babel/plugin-proposal-private-methods": "latest",
		"@babel/plugin-proposal-throw-expressions": "latest",
		"@babel/plugin-syntax-dynamic-import": "latest",
		"@babel/plugin-syntax-import-meta": "latest",
		"@babel/plugin-transform-flow-strip-types": "latest",
		"@babel/plugin-transform-runtime": "latest",
		"@babel/preset-env": "latest",
		"@babel/preset-flow": "latest",
		"@babel/runtime": "latest",
		"babel-loader": "latest",
		"babel-minify": "latest",
		"babel-minify-webpack-plugin": "latest",
		"circular-dependency-plugin": "latest",
		"copy-webpack-plugin": "latest",
		"copyfiles": "latest",
		"diff": "latest",
		"duplicate-package-checker-webpack-plugin": "latest",
		"eslint-plugin-flowtype": "latest",
		"flow-bin": "latest",
		"html-webpack-plugin": "latest",
		"http-server": "latest",
		"parallel-webpack": "latest",
		"rimraf": "latest",
		"script-ext-html-webpack-plugin": "latest",
		"webpack": "latest",
		"webpack-bundle-analyzer": "latest",
		"webpack-cli": "latest",
		"webpack-dev-server": "latest",
		"webpack-manifest-plugin": "latest",
		"webpack-merge": "latest",
		"webpack-messages": "latest",
		"optimize-css-assets-webpack-plugin": "latest",
		"webpack-polyfill-injector": "latest",
		"prettier": "latest",
