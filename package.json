{
	"name": "ecmascript-toolkit",
	"short_name": "etk",
	"main": "./src/index.js",
	"version": "1.0.0-alpha.2",
	"description": "",
	"scripts": {
		"start": "yarn serve",
		"start:analyze": "yarn analyze",
		"start:prod": "yarn prod",
		"build": "yarn parallel",
		"parallel": "yarn parallel-webpack",
		"serve": "yarn webpack-dev-server",
		"prod": "yarn webpack-dev-server --config=./config/webpack.prod.js",
		"analyze": "yarn webpack-dev-server --config=./config/webpack.analyze.js",
		"analyze:static": "yarn parallel --config=./config/webpack.analyze.static.js",
		"analyze:manual": "yarn parallel-webpack --profile --json > stats.json && yarn webpack-bundle-analyzer",
		"watch": "yarn build --watch"
	},
	"author": "Ryan Tyler Spice-Finnie <spice.ryan@hotmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"@babel/plugin-proposal-decorators": "^7.7.0",
		"@babel/plugin-proposal-export-default-from": "^7.5.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.5.2",
		"@babel/plugin-proposal-function-sent": "^7.7.0",
		"@babel/plugin-proposal-json-strings": "^7.2.0",
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-proposal-optional-chaining": "^7.6.0",
		"@babel/plugin-proposal-throw-expressions": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@babel/plugin-transform-flow-strip-types": "^7.6.3",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-flow": "^7.0.0",
		"@babel/runtime": "^7.7.2",
		"babel-loader": "^8.0.6",
		"babel-minify-webpack-plugin": "^0.3.1",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"flow-bin": "^0.112.0",
		"http-server": "^0.11.1",
		"parallel-webpack": "^2.4.0",
		"webpack": "^5.0.0-beta.6",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0",
		"webpack-merge": "^4.2.2"
